{
  "name": "prestashop/stanconnect",
  "description": "PrestaShop module stanconnect",
  "homepage": "https://stan-business.fr",
  "license": "AFL-3.0",
  "authors": [
    {
      "name": "Brightweb editeur de Stan",
      "email": "jonathan@stan-app.fr"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "stan-business/stan-php": "master-dev",
    "php-http/guzzle5-adapter": "^2.0",
    "php-http/discovery": "^1.14",
    "php-http/message": "^1.13",
    "guzzlehttp/psr7": "^2.4"
  },
  "autoload": {
    "classmap": [
      "controllers/",
      "stanconnect.php"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "prepend-autoloader": false,
    "classmap-authoritative": true,
    "platform-check": false
  },
  "type": "prestashop-module",
  "require-dev": {
    "prestashop/php-dev-tools": "^4.2",
    "phpstan/phpstan": "^1.8"
  }
}
